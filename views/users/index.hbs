<h1>Users</h1>

<ul>
    <div class="row">
        <div class="col s12 m6">
            {{#each users}}
            <li>
                <a href="/users/{{_id}}">
                    <p>{{username}}</p>
                </a>
                <img src="{{photoUrl}}">
                <p>Favorite Movie Quote: {{favoriteMovieQuote}}</p>
            </li>
            {{/each}}
        </div>
    </div>
    <div class="addUser">
        <a href="users/new" class="btn-floating btn-large waves-effect waves-light red">
            <i class="material-icons">add</i>
        </a>

    </div>

</ul>