<h1>Users</h1>

<ul>
    {{#each users}}
    <div class="row">
        <div class="col s12 m6">
            <li>
                <a href="/users/{{_id}}">
                    <p>{{username}}</p>
                </a>
                <img src="{{photoUrl}}">
                <p>Favorite Movie Quote: {{favoriteMovieQuote}}</p>
            </li>
        </div>
    </div>
    {{/each}}
</ul>