<h1>Users</h1>

<ul>
    {{#each users}}
    <div class="row">
    <li class="col s12 m6">
        <a href="/users/{{_id}}">
            <p>{{username}}</p>
        </a>
        <img src="{{photoUrl}}">
        <p>Favorite Movie Quote: {{favoriteMovieQuote}}</p>
    </li>
    </div>
    {{/each}}
</ul>