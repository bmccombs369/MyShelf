<h1>{{user.username}}'s Catalogs</h1>

<ul>
    {{#each catalogs}}
    <li>
        <a href="/users/{{_id}}/catalogs/{{this._id}}/movies">
            <p>{{name}}</p>
    </li>
    {{/each}}
</ul>

<div class="addCatalog">
    <a href="users/{{user._id}}/catalogs/new" class="btn-floating btn-large waves-effect waves-light red">
        <i class="material-icons">add</i>
    </a>
</div>